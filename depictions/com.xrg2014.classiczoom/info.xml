<package>
	<id>com.xrg2014.classiczoom</id>
	<name>Classic Zoom</name>
	<version>1.0.0-1</version>
	<compatibility>
		<firmware>
			<miniOS>13.0</miniOS>
		</firmware>
	</compatibility>
	<dependencies>
		<package>com.anemonetheming.anemone | com.spark.snowboard</package>
	</dependencies>
	<shortDescription>This theme brings back the classic Zoom icon. Firmware should be greater than 13.0</shortDescription>
	<descriptionlist>
		<description>This is a theme that brings back the old Zoom icon.</description>
		<description>The icon is from: https://cdn-icons-png.freepik.com/256/14034/14034521.png?semt=ais_hybrid</description>
	</descriptionlist>
	<screenshots>
		<screenshot>
			<description>This is the Zoom icon</description>
			<image>zoom.jpg</image>
		</screenshot>		
	</screenshots>
	<links>
		<link>
			<name>/r/jailbreak</name>
			<url>https://www.reddit.com/r/jailbreak</url>
			<iconclass>fa fa-reddit</iconclass>
		</link>
	</links>
</package>
